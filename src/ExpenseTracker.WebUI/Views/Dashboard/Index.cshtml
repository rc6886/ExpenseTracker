@{ 
    Layout = "_Layout";
}

@model ExpenseTracker.Core.ViewModels.Dashboard.DashboardTotalsViewModel



<div id="dashboardIndex">
    <div class="row">
        @Html.Partial("_AddTransaction")
    </div>
    <div class="row">
        @Html.Partial("_DashboardTotals", Model)
    </div>
    <div class="row">
        @Html.Partial("_DashboardPieChart")
    </div>
    <div class="row">
        <div class="col-lg-12">
            @Html.Partial("_DashboardTransactionTable")
        </div>
    </div>
</div>

<script src="//rawgithub.com/indrimuska/jquery-editable-select/master/dist/jquery-editable-select.min.js"></script>
<link href="//rawgithub.com/indrimuska/jquery-editable-select/master/dist/jquery-editable-select.min.css" rel="stylesheet">
<script src="~/js/app/dashboardIndex.js"></script>



